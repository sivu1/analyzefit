Notebook[{

Cell[CellGroupData[{
Cell["Linear Regression", "Title",
 CellChangeTimes->{{3.68415687638097*^9, 3.684156887576745*^9}}],

Cell[TextData[{
 "Whenever we perform a linear regression on some data, we might get carried \
away by good values of ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["R", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " or p. However, such single valued quantities don\[CloseCurlyQuote]t tell \
the whole story. "
}], "Text",
 CellChangeTimes->{{3.684156910565707*^9, 3.684156984726448*^9}}],

Cell[CellGroupData[{

Cell["Temperature Model", "Section",
 CellChangeTimes->{{3.684156988321323*^9, 3.6841569940810413`*^9}}],

Cell["We will create a model that is a function of temperature:", "Text",
 CellChangeTimes->{{3.68415704315423*^9, 3.6841570627206287`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"c", " ", "=", " ", 
   RowBox[{"RandomReal", "[", 
    RowBox[{"{", 
     RowBox[{"0", ",", "25"}], "}"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"y", " ", "=", " ", 
   RowBox[{"RandomReal", "[", 
    RowBox[{"{", 
     RowBox[{"0", ",", "100"}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"M", "[", "T_", "]"}], ":=", " ", 
   RowBox[{
    RowBox[{"c", " ", "T"}], " ", "+", " ", "y"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.684156998665221*^9, 3.684157034393716*^9}}],

Cell["We will now add random noise to the model:", "Text",
 CellChangeTimes->{{3.6841570695048647`*^9, 3.68415708010362*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Nsamples", " ", "=", " ", "250"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Tsamples", " ", "=", " ", 
   RowBox[{
    RowBox[{"RandomReal", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"0", ",", "100"}], "}"}], ",", " ", "Nsamples"}], "]"}], "//", 
    "Sort"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"noise", " ", "=", " ", "25"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Msamples", " ", "=", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"M", "/@", "Tsamples"}], ")"}], "+", 
    RowBox[{"RandomVariate", "[", 
     RowBox[{
      RowBox[{"NormalDistribution", "[", 
       RowBox[{"0", ",", 
        RowBox[{"noise", " ", "c"}]}], "]"}], ",", "Nsamples"}], "]"}]}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.684157084070034*^9, 3.68415719365844*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"MTdata", " ", "=", " ", 
   RowBox[{
    RowBox[{"Riffle", "[", 
     RowBox[{"Tsamples", ",", " ", "Msamples"}], "]"}], " ", "//", 
    RowBox[{
     RowBox[{"Partition", "[", 
      RowBox[{"#", ",", "2"}], "]"}], "&"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.684157197466837*^9, 3.684157234953063*^9}}],

